<h>ITINARY</h>


<% @albums.each do |album| %>
  <p>Artist: <%= Artist.find( album.artist_id ).name%> </p>
  <p>Title: <%= album.title %></p>
  <p>Genre: <%= album.genre %></p>
  <p>Quantity: <%= album.quantity %></p>
  <p>Stock: <%= album.stock_level_checker%> </p>
  <p><a href="/albums/<%= album.id %>">Additional Information</a></p>
  <hr />
  <br>
  <% end %>
